{
  "name": "am-vscode",
  "displayName": "Am Test",
  "description": "Am Test support for Visual Studio Code.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Programming Languages"
